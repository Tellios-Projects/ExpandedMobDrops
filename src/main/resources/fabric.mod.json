{
  "schemaVersion": 1,
  "id": "expandedmobdrops",
  "version": "${version}",

  "name": "Expanded Mob Drops",
  "description": "Adds more types of animal meat.",
  "authors": [
    "Leah (@leafenzio), Eliza (@tellioaridoitsu)"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/expanded-mob-drops",
    "sources": "https://github.com/Tellios-Projects/ExpandedMobDrops"
  },

  "license": "MIT",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.leafenzo.expandedmobdrops.ModInit"
    ],
    "client": [
      "net.leafenzo.expandedmobdrops.ModClientInit"
    ],
    "fabric-datagen": [
      "net.leafenzo.expandedmobdrops.datageneration.ModDataGenerator"
    ]
  },
  "accessWidener": "expandedmobdrops.accesswidener",
  "mixins": [
    "expandedmobdrops.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.14",
    "fabric-api": "*",
    "minecraft": "~1.20.1",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
